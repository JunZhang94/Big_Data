* {
	margin: 0;
	padding: 0;
	margin-bottom : 0px;
}

body {
	min-width: 1024px;
	width: expression(( documentElement.clientWidth <   1024)?  "1024px":  "auto"
		);
	margin: 0;
	padding: 0;
	font-size: 12px;
	line-height: 24px;
	color: #444;
}

li {
	list-style: none;
}

ul,li,p {
	margin: 0;
	padding: 0;
}

.clearfix:after {
	visibility: hidden;
	display: block;
	font-size: 0;
	content: " ";
	clear: both;
	height: 0;
}

.x-grid-record-red table{
   background: #E6D1E3;
}
.x-grid-record-yellow table{
   background: #FF9900 !important;
}

.clearfix {
	zoom: 1;
}

html {
