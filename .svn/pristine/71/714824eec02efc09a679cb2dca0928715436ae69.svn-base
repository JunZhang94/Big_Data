/******************************************
*FOR LAYOUT
*******************************************/
/*****About toggler*****/
.ui-layout-center{
	border:none;
	border-right:1px solid #54BFDF;
	border-bottom:1px solid #54BFDF;
}
.ui-layout-north{
	border:1px solid #54BFDF;
}
.ui-layout-west{
	border:none;
	border-left:1px solid #54BFDF;
	border-bottom:1px solid #54BFDF;
}
.ui-layout-toggler-north-open {
	background:#FFF url(images/arrows.png) no-repeat 0 -54px;	
}
.ui-layout-toggler-north-closed{
	background:#FFF url(images/arrows.png) no-repeat 0 -62px;
} 
.ui-layout-toggler-south-open {
	background:#FFF url(images/arrows.png) no-repeat 0 -62px;
}
.ui-layout-toggler-south-closed {
	background:#FFF url(images/arrows.png) no-repeat 0 -54px;
}
.ui-layout-toggler-west-open {
	background:#FFF url(images/arrows.png) no-repeat 0 0;
}
.ui-layout-toggler-west-closed {
	background:#FFF url(images/arrows.png) no-repeat -6px 0;
}
.ui-layout-toggler-east-open {
	background:#FFF url(images/arrows.png) no-repeat -6px 0;
}
.ui-layout-toggler-east-closed {
	background:#FFF url(images/arrows.png) no-repeat 0 0;
}
/*****About Resizer*****/
.ui-layout-resizer{
	border:none;
	background-color:#54BFDF;
}
	.ui-layout-resizer-south{
		background: #D7F1F8 url(images/arrows.png) repeat-x scroll 0 -70px; ;
	}
	.ui-layout-resizer-dragging-limit {	
			background: #E18D8D;
	}

/*****About Pane*****/
.ui-layout-pane-north{
	border-bottom:1px solid #037397;
}

/******************************************
*For contextMenu
*******************************************/
.context-menu-theme-default {
	border:1px solid #54BFDF;
	background-color:#F7FDFF;
}
.context-menu-theme-default .context-menu-item {
	color:#666;
}
.context-menu-theme-default .context-menu-item label{}
.context-menu-theme-default .context-menu-separator {
	border-top:1px solid #808080;
	border-bottom:1px solid white;
}
.context-menu-theme-default .context-menu-item-disabled {
	color:#808080;
}
.context-menu-theme-default .context-menu-item .context-menu-item-inner {
	background:none no-repeat fixed 999px 999px; /* Make sure icons don't appear */
}
.context-menu-theme-default .context-menu-item-hover {
	background-color:#69B9DA;
	color:white;
}
.context-menu-theme-default .context-menu-item-disabled-hover {
	background-color:#0A246A;
}
.left{width:25%;float:left;}
.right{width:75%;height:100%;float:right;}
