/**layout and border*/
.x-layout-split {
  background-color: #DCDCDC;
  width: 7px;
  height : 7px;
}

.ext-strict .ext-ie6 .x-layout-split{/**fix for ie6 mini mode bug*/
	background-color:#DCDCDC!important;
	filter:none;
}
.x-layout-collapsed {
  background-color: #DCDCDC;
  border-color: #98C0F4;
}
.x-layout-cmini-west, .x-layout-cmini-east {
  padding: 0;
  width: 7px !important;
}
.x-layout-cmini-north, .x-layout-cmini-south {
  height: 7px !important;
  padding: 0;
}
.x-layout-split-south .x-layout-mini {
 background: url("images/down_bar_hover.jpg") no-repeat scroll 0 0 transparent!important;
  border-bottom: 1px solid #BCBCBC;
  height: 7px;
  width : 43px;
}
.x-layout-split-west .x-layout-mini {
  background: url(images/fold.gif) no-repeat -34px -8px;
 /* background: url("images/left_bar_hover.jpg") no-repeat scroll 0 0 transparent!important;*/
  border-right: 1px solid #BCBCBC;
  width: 8px;
  height: 52px;
}
.x-layout-split-east .x-layout-mini {
  background: url("images/right_bar_hover.jpg") no-repeat scroll 0 0 transparent!important;
  border-right: 1px solid #BCBCBC;
  width: 8px;
  height: 52px;
}
.x-layout-cmini-west .x-layout-mini {
  /*background: url("images/right_bar_hover.jpg") no-repeat scroll 0 0 transparent!important;*/
  background: url(images/fold.gif) no-repeat -60px -8px;
  border-right: 1px solid #BCBCBC;
  width: 8px;
  height: 52px;
}
.x-layout-cmini-east .x-layout-mini {
  background: url("images/left_bar_hover.jpg") no-repeat scroll 0 0 transparent;
  border-right: 1px solid #BCBCBC;
  width: 8px;
  height: 52px;
}
.x-layout-cmini-south .x-layout-mini {
  background: url("images/up_bar_hover.jpg") no-repeat scroll 0 0 transparent;
  border-right: 1px solid #BCBCBC;
  height: 7px;
  width : 43px;
}
.x-layout-mini {
  opacity: 0.8;
  filter:alpha(opacity=80);
}
.x-layout-mini-over, .x-layout-collapsed-over .x-layout-mini{
    opacity:1!important;
    filter:alpha(opacity=100);
    -moz-opacity:1!important;
}
