	background: rgb(210, 240, 255);
}
.pagination table{
	float: right;
}
.pagination-info{
	float : left;
	padding-top: 0px;
	padding-left:5px;
	line-height: 30px;
}

.panel-header{
	background: url(images/alarm/alarm.gif) repeat-x scroll 0 -37px transparent;
	color: white;
	border-color: #bbbbbb;
}
.panel-body-noheader{
	border:0px;
	border-color: #bbbbbb;
}

/**tips*/
.validatebox-invalid{
	background:#FFFFEE url(images/error.gif) no-repeat right 1px;
}
.validatebox-tip-content{
	border:1px solid red;
	background:white;
	color: red;
}
.validatebox-tip-pointer{
	background:url('images/validatebox_pointer.gif') no-repeat left top;
	display:inline-block;
	width:10px;
	height:19px;
	position:absolute;
	left:1px;
	top:0px;
	z-index:9900003;
}
/**window*/
.window {
	border:0px;
	background: url("images/layer/top.gif") repeat-x scroll 0 0 transparent;
	background-color: #0a8ece;
}
.window-shadow{
	background:none;
	-moz-box-shadow: none;
	-webkit-box-shadow: none;
}
/**button*/
a.l-btn-plain {
    background: url("images/btn.gif") repeat-x scroll 0 -35px transparent;
    border: 1px solid #03A1D8;
    height: 24px;
    padding: 0 10px;
    margin: 0px;
}
a:hover.l-btn-plain{
	background: url("images/btn.gif") repeat-x scroll 0 -70px transparent;
    border: 1px solid #05C4E9;
    border-radius:0px;
}
a.l-btn span.l-btn-left{
	padding:4px 0px 4px 1px;
}
/**pagingtoolbar*/
.pagination a.l-btn-plain {
	background:transparent;
	padding-right:5px;
	border:1px solid transparent;
	_border:0px solid #efefef;
	_padding:1px 6px 1px 1px;
}
.pagination a:hover.l-btn-plain {
	border:1px solid #7eabcd;
	background:url('images/button_plain_hover.png') repeat-x left bottom;
	_padding:0px 5px 0px 0px;
	-moz-border-radius:3px;
	-webkit-border-radius: 3px;
}
