/**gridpanel*/
.datagrid-toolbar{
	background: url();
}
.datagrid-view{
	line-height:18px;
}
.datagrid-header{
	background:url(images/table_bg.jpg);
}
.datagrid-header td{
	font-weight: bold;
}
.datagrid-header td{
	background:url(images/table_bg.jpg);
}
.datagrid-row-alt{
	background: rgb(241, 241, 241);
}
