/**tabpanel*/
.x-tab-strip-wrap {
	overflow: hidden;
	position: relative;
}

.x-tab-panel-header,.x-tab-panel-footer {
	background-color: white;
	border: 0px;
}

.x-tab-strip-top .x-tab-right,.x-tab-strip-top .x-tab-left,.x-tab-strip-top .x-tab-strip-inner
	{
	background: transparent;
	
}

ul.x-tab-strip-top {
	background: url("images/bg.gif") repeat-x scroll 0 -7px transparent;
	padding-left: 5px;
	border-bottom-width: 0px;
}

ul.x-tab-strip li {
	background: transparent;
	margin-left: 0px;
	height: 25px;
	width: 80px;
}

.x-tab-strip-top .x-tab-right {
	padding-left: 10px;
	width: 80px;
	text-align: center;
}

.x-tab-strip-top .x-tab-strip-active .x-tab-right {
	background: repeat-x scroll 0 -40px transparent;
	padding-left: 0px;
	width: 80px;
	height: 26px;
	text-align: center;
	color: #1088DB;
}

.x-tab-strip-active {
	border-color: #B5B5B5;
	border-style: solid;
	margin-left: -1px;
	margin-right: -1px;
	border-width: 1px 1px 0 1px;
}

.x-tab-strip-active a.x-tab-right {
	border-color: #B5B5B5;
	border-style: solid;
	margin-left: -1px;
	margin-right: -1px;
	border-width: 0px 1px;
}

.x-tab-strip-top .x-tab-strip-active .x-tab-right span.x-tab-strip-text
	{
	color: #1088DB;
}

.x-tab-strip-top .x-tab-strip-active .x-tab-strip-inner {
	background: url("images/bg.gif") repeat-x scroll 0 -40px transparent;
	width: 80px;
}

.x-tab-strip span.x-tab-strip-text {
	color: #666666;
	font-weight: bold;
}

.x-tab-panel-body-top {
	border-bottom-width: 0px;
	border-color: #BBBBBB;
}

.x-tab-strip-top .x-tab-left {
	_padding-right: 0px;
}

#ocxToolbar .x-btn {
	border: 0;
}

.gridPanelBottomBar .x-toolbar {
  background: none repeat scroll 0 0 #D2F0FF;
  padding: 0px;
}
.gridPanelBottomBar .x-btn {
  border: none;
}

#picturewesttabpanel .x-btn {
	background: url(images/btn.gif);
	background-position: 0 -61px;
	height: 18px;
	border: 1px solid #1D95DD;
	padding: 0 10px;
	color:white
}
#picturewesttabpanel .x-btn button{
	color:white
}

.x-window .x-btn {
	background: url(images/btn.gif);
	background-position: 0 -61px;
	height: 18px;
	border: 1px solid #1D95DD;
	padding: 0 10px
}

.x-window .x-btn button,.x-date-picker .x-btn button {
	color: white
}

.x-window .x-btn-over {
	background-position: 0 -94px;
}